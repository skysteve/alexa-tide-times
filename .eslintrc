{
  "extends": "airbnb/base",
  "rules" : {
    "no-case-declarations": 0,
    "camelcase": 0,
    "class-methods-use-this": 0,
    "comma-dangle" : 0,
    "import/prefer-default-export": 0,
    "no-unused-vars" : 1,
    "object-shorthand" : 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-console": 0,
    "arrow-parens": 0,
    "no-plusplus": 0
  }
}
